{
    "blocks": [{
            "type": "prompt",
            "alignment": "left",
            "segments": [{
                "type": "python",
                "style": "plain",
                "foreground": "white",
                "properties": {
                    "prefix": "(",
                    "postfix": ")",
                    "display_version": false
                }
            }]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [{
                    "type": "text",
                    "style": "plain",
                    "foreground": "yellow",
                    "properties": {
                        "prefix": "",
                        "text": "<green>\u276F</><#BF616A>\u276F</><blue>\u276F</>"
                    }
                },
                {
                    "type": "root",
                    "style": "plain",
                    "foreground": "#CECE04"
                },
                {
                    "type": "session",
                    "style": "plain",
                    "foreground": "green",
                    "properties": {
                        "display_host": false,
                        "prefix": " ",
                        "postfix": " ",
                        "user_info_separator": "",
                        "display_user": true
                    }
                },
                {
                    "type": "path",
                    "style": "plain",
                    "invert_powerline": false,
                    "foreground": "blue",
                    "properties": {
                        "prefix": "<#BF616A>in</> ",
                        "style": "folder"
                    }
                },
                {
                    "type": "git",
                    "style": "plain",
                    "invert_powerline": false,
                    "foreground": "#DA70D6",
                    "properties": {
                        "branch_icon": " <#DA70D6><b>î‚  </b></>",
                        "display_stash_count": true,
                        "display_status": false,
                        "prefix": "<#BF616A>on</> "
                    }
                },
                {
                    "type": "time",
                    "style": "plain",
                    "foreground": "#E5C07B",
                    "properties": {
                        "prefix": "<#BF616A>at</> [",
                        "postfix": "]"
                    }
                },
                {
                    "type": "exit",
                    "style": "plain",
                    "foreground": "#BF616A",
                    "properties": {
                        "prefix": " C:",
                        "always_numeric": true
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [{
                "type": "text",
                "style": "plain",
                "foreground": "lightRed",
                "properties": {
                    "prefix": "",
                    "text": "$",
                    "postfix": ""
                }
            }]
        }
    ],
    "final_space": true
}